<block wx:if="{{$taroCompReady}}">
    <view>
        <view style="{{anonymousState__temp}}"></view>
        <at-nav-bar compid="{{$compid__664}}"></at-nav-bar>
        <view class="auto-control">
            <sub-title compid="{{$compid__665}}"></sub-title>
            <view class="factor-area">
                <picker mode="multiSelector" range="{{factor_selector}}" bindchange="onFactorChange" data-e-change-so="this">
                    <view class="at-row">
                        <view class="at-col">
                            <cl-icon compid="{{$compid__666}}"></cl-icon>
                        </view>
                        <view class="at-col at-col__offset-2" style="padding-top:2px">{{factor_obj}}</view>
                        <view class="at-col at-col__offset-0" style="padding-top:2px">{{factor_action.length > 1 ? factor_action + '时' : '无'}}</view>
                    </view>
                </picker>
            </view>
            <sub-title compid="{{$compid__667}}"></sub-title>
            <view class="factor-area" style="padding: 0 32rpx;">
                <at-list>
                    <at-list-item compid="{{$compid__668}}"></at-list-item>
                </at-list>
            </view>
            <view class="factor-area" style="padding: 12rpx 32rpx; margin-top:0">
                <view class="at-row">
                    <view class="at-col" style="padding-top:2px">运行前询问</view>
                    <view class="at-col at-col__offset-12">
                        <cl-switch compid="{{$compid__669}}"></cl-switch>
                    </view>
                </view>
            </view>
        </view>
    </view>
